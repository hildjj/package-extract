{
  "name": "package-extract",
  "version": "3.1.4",
  "description": "Extract fields from package.json into an ES6 module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hildjj/package-extract.git"
  },
  "homepage": "https://github.com/hildjj/package-extract",
  "bugs": {
    "url": "https://github.com/hildjj/package-extract/issues"
  },
  "type": "module",
  "bin": {
    "package-extract": "bin/package-extract.js"
  },
  "main": "lib/index.js",
  "types": "types/lib/index.d.ts",
  "files": [
    "bin",
    "lib",
    "types"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "c8 node --test",
    "ver": "node ./bin/package-extract.js -s",
    "build": "npm run ver && npm run types && npm test && npm run lint && npm pack --dry-run",
    "types": "tsc",
    "version": "npm run ver && npm run build && git add package.js types/package.d.ts"
  },
  "keywords": [
    "package.json",
    "version",
    "extract",
    "multiple",
    "code",
    "import",
    "es6",
    "genversion"
  ],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "dependencies": {
    "commander": "14.0.3",
    "semver": "7.7.4"
  },
  "devDependencies": {
    "@cto.af/eslint-config": "6.2.4",
    "@eslint/markdown": "7.5.1",
    "@types/node": "25.3.0",
    "@types/semver": "7.7.1",
    "c8": "10.1.3",
    "eslint": "10.0.1",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.30.1",
  "pnpm": {
    "overrides": {
      "eslint@10.0.1>ajv": "6.14.0",
      "enhance-visitors@1.0.0>lodash": "4.17.23",
      "c8@10.1.3>test-exclude": "7.0.2",
      "glob@10.5.0>minimatch": "10.2.2"
    }
  },
  "engines": {
    "node": ">=20"
  }
}
