{
  "name": "package-extract",
  "version": "1.0.3",
  "description": "Extract fields from package.json into an ES6 module",
  "homepage": "https://github.com/hildjj/package-extract",
  "bugs": {
    "url": "https://github.com/hildjj/package-extract/issues"
  },
  "type": "module",
  "bin": "bin/package-extract.js",
  "main": "lib/index.js",
  "types": "types/lib/index.d.ts",
  "scripts": {
    "lint": "eslint . --ext js",
    "test": "ava",
    "coverage": "c8 -r lcov npm run test",
    "build": "./bin/package-extract.js && npm run types",
    "types": "tsc",
    "version": "npm run build && git add package.js types/package.d.ts"
  },
  "keywords": [
    "package.json",
    "version",
    "extract",
    "multiple",
    "code",
    "import",
    "es6",
    "genversion"
  ],
  "author": "Joe Hildebrand <joe-github@cursive.net>",
  "license": "MIT",
  "dependencies": {
    "commander": "12.1.0"
  },
  "devDependencies": {
    "@cto.af/eslint-config": "4.0.2",
    "@types/node": "20.14.2",
    "ava": "6.1.3",
    "c8": "10.1.1",
    "eslint": "8.57.0",
    "eslint-plugin-ava": "14.0.0",
    "eslint-plugin-node": "11.1.0",
    "typescript": "5.4.5"
  },
  "pnpm": {
    "overrides": {
      "semver": ">=7.5.3",
      "word-wrap": "npm:@aashutoshrathi/word-wrap"
    }
  },
  "overrides": {
    "semver": ">=7.5.3",
    "word-wrap": "npm:@aashutoshrathi/word-wrap"
  },
  "packageManager": "pnpm@9.3.0",
  "engines": {
    "node": ">=18"
  }
}
